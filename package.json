{
  "name": "isomorphic-universal-react-redux-boilerplate-seed",
  "version": "0.1.0",
  "description": "",
  "main": "./bin/server.js",
  "scripts": {
    "dev": "concurrently --kill-others \"better-npm-run webpack-dev-server\" \"better-npm-run dev-server\" \"better-npm-run dev-api\"",
    "dev-api": "better-npm-run dev-api",
    "dev-server": "better-npm-run dev-server",
    "webpack-dev-server": "better-npm-run webpack-dev-server",
    "start-api": "better-npm-run start-api",
    "start-server": "better-npm-run start-server",
    "build": "better-npm-run build",
    "start": "npm run build && concurrently --kill-others \"better-npm-run start-server\" \"better-npm-run start-api\""
  },
  "betterScripts": {
    "dev-server": {
      "command": "nodemon ./bin/server.js",
      "env": {
        "NODE_PATH": "./",
        "NODE_ENV": "development"
      }
    },
    "dev-api": {
      "command": "nodemon ./bin/api.js",
      "env": {
        "NODE_PATH": "./",
        "NODE_ENV": "development"
      }
    },
    "webpack-dev-server": {
      "command": "node ./bin/webpack-dev-server.js",
      "env": {
        "NODE_PATH": "./",
        "NODE_ENV": "development"
      }
    },
    "start-server": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./",
        "NODE_ENV": "production"
      }
    },
    "start-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./",
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "./"
      }
    }
  },
  "repository": {
    "type": "git"
  },
  "author": "Sankalp Lakhina <sankalp.lakhina@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.22.0",
    "better-npm-run": "0.0.14",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "http-proxy": "^1.15.1",
    "isomorphic-fetch": "^2.2.1",
    "jkstra": "0.0.6",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.0",
    "morgan": "^1.7.0",
    "postcss-loader": "^1.2.2",
    "pretty-error": "^2.0.1",
    "react": "^15.4.2",
    "react-bind-handlers": "^1.0.5",
    "react-dom": "^15.4.2",
    "react-dropdown": "^1.3.3",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-connect": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-isomorphic-tools": "^2.5.8"
  },
  "devDependencies": {
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "nodemon": "^1.10.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  }
}
